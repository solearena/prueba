package ejemplo2;

public class Messi {

	private String nacionalidad;
	private Barcelona barcelona;
	
	public Messi() {
		
	}

	public Messi(String nacionalidad, Barcelona barcelona) {
		super();
		this.nacionalidad = nacionalidad;
		this.barcelona = barcelona;
	}

	public String getNacionalidad() {
		return nacionalidad;
	}

	public void setNacionalidad(String nacionalidad) {
		this.nacionalidad = nacionalidad;
	}

	public Barcelona getBarcelona() {
		return barcelona;
	}

	public void setBarcelona(Barcelona barcelona) {
		this.barcelona = barcelona;
	}

	@Override
	public String toString() {
		return "Messi [nacionalidad=" + nacionalidad + ", barcelona=" + barcelona + "]";
	}

	
}
