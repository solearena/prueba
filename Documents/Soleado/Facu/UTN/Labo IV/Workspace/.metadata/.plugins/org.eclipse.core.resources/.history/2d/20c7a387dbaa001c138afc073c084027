package dominio;

public class ArticuloGimnasio extends Articulo{
	
	private int peso;
	
	//constructores
	public ArticuloGimnasio() {
		super();
	}

	public ArticuloGimnasio(String nombre, int peso) {
		super(nombre);
		this.peso = peso;
	}



	@Override
	public String toString() {
		return "ArticuloGimnasio [peso=" + peso + "]" + super.toString();
	}

	public int getPeso() {
		return peso;
	}

	public void setPeso(int peso) {
		this.peso = peso;
	}

}
